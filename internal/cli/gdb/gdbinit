set height 0
set width 0

set disassembly-flavor intel

set history filename ~/.gdb_history
set history save

set verbose off

set $COLOREDPROMPT = 1
set $SETCOLOR1STLINE = 0
set $SHOWOBJECTIVEC = 1
set $SHOWCPUREGISTERS = 1
set $SHOWSTACK = 1
set $SHOWDATAWIN = 1
set $SHOWREGCHANGES = 1
set $X86FLAVOR = 0
set $USECOLOR = 1
source /home/initbar/.gdb.d/pwndbg/gdbinit.py
